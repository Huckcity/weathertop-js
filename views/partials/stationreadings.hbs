<table class='table table-hover'>
  <thead>
    <tr>
      <th scope='col'>#</th>
      <th scope='col'>Code</th>
      <th scope='col'>Temp</th>
      <th scope='col'>Wind Speed</th>
      <th scope='col'>Wind Direction</th>
      <th scope='col'>Pressure</th>
    </tr>
  </thead>
  <tbody>
    {{#each readings}}
      <tr>
        <th scope='row'>{{@key}}</th>
        <td>{{code}}</td>
        <td>{{temperature}}</td>
        <td>{{windSpeed}}</td>
        <td>{{windDirection}}</td>
        <td>{{pressure}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>