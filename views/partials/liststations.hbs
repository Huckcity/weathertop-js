{{#each stations}}
<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">{{name}}</h5>
    <h6 class="card-subtitle mb-2 text-muted">{{lat}} - {{lng}}</h6>
    <a href="#" class="card-link">View</a>
    <a href="/stations/delete/{{_id}}" class="card-link">Delete</a>
  </div>
</div>
{{/each}}